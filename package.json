  "bugs": {
    "url": "https://github.com/livechat/agentapp-widget-sdk/issues"
  },
  "homepage": "https://github.com/livechat/agentapp-widget-sdk#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "rollup": "^0.55.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-uglify": "^3.0.0"
  }

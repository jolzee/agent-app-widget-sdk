{
  "name": "@livechat/agent-app-widget-sdk",
  "version": "1.2.1",
  "description": "SDK for LiveChat inapp widget API",
  "main": "dist/agentapp-widget.js",
  "scripts": {
    "build:umd:min":
    "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -i src/index.js -o dist/agentapp-widget.min.js",
